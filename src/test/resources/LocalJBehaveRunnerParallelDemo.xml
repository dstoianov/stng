<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
    <listeners>
        <listener class-name="organized.chaos.LocalWebDriverListener" />
    </listeners>
    <test name="Tests in FF" junit="true">
        <parameter name="browserName" value="firefox" />
        <!-- If you want to be more specific, replace this with a list of test classes you'd like to run -->
        <classes>
            <class name="org.stng.tests.ModifiedOfficialJBehaveTutorialICanToggleACell"/>
        </classes>
    </test>
    <test name="Tests in Chrome" junit="true">
        <parameter name="browserName" value="chrome" />
        <classes>
            <class name="org.stng.tests.ModifiedOfficialJBehaveTutorialICanToggleACell"/>
        </classes>
    </test>
</suite>